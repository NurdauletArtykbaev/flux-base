{
  "name": "nurdaulet/flux-base",
  "description": "Base provider",
  "license": "proprietary",
  "authors": [
    {
      "name": "Nurdaulet",
      "email": "artykbaev.nurdaulet@mail.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nurdaulet\\FluxBase\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "spatie/laravel-translatable": "^6.5",
    "staudenmeir/laravel-adjacency-list": "^1.0",
    "filament/filament": "^2.16",
    "filament/spatie-laravel-translatable-plugin": "^2.17",
    "stevebauman/location": "*",
    "symfony/http-client": "^6.4",
    "geocoder-php/yandex-provider": "^4.5",
    "psr/http-client": "^1.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.11"
  },
  "extra": {
    "laravel": {
      "aliases": {
          "TextConverter": "Nurdaulet\\FluxBase\\Facades\\TextConverter",
          "StringFormatter": "Nurdaulet\\FluxBase\\Facades\\StringFormatter"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
